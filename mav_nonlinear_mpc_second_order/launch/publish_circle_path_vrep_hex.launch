<launch>
  <arg name="mav_name" default="vrep_hex" />
  <arg name="namespace" default="$(arg mav_name)"/>
  <arg name="uav_num" default="1"/>
  <param name="use_sim_time" value="true" />

  <node pkg="mav_nonlinear_mpc_second_order" name="publish_full_circle_path" args="$(arg mav_name) $(arg uav_num)" output="screen" type="publish_full_circle_path.py"/>
</launch>